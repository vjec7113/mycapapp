{
    "welcomeFile": "managepo/webapp/index.html",
    "authenticationMethod": "route",
    "sessionTimeout": 100,
    "routes":[
        {
            "source": "^/managepo/(.*)",
            "target": "$1",
            "localDir": "managepo/",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/webapp/(.*)",
            "target": "$1",
            "localDir": "webapp/",
            "authenticationType": "xsuaa"
        },
        {
            "source": "^/(.*)$",
            "destination": "srv-api",
            "authenticationType": "xsuaa"
        }
    ],
    "logout": { 
        "logoutEndpoint": "/logout", 
        "logoutPage": "https://www.google.com"
      }
}